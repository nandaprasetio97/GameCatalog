<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nandaprasetio.gamecatalog">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".GameCatalogApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GameCatalog">
        <activity android:name="com.nandaprasetio.gamecatalog.presentation.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.nandaprasetio.gamecatalog.presentation.activity.GameDeveloperDetailActivity" />
        <activity android:name="com.nandaprasetio.gamecatalog.presentation.activity.GenreDetailActivity" />
    </application>

</manifest>